<div id="rateus" v-cloak>

  <div class="container-fluid contactCover">
    <div class="container mt-4">
      <div class="ratefrm mt-5">
      <h1 class="mt-4 mb-3">Rate Us</h1>
      <form>
        <div class="form-group mt-2">
          <input type="name" class="form-control" id="exampleFormControlInput1" placeholder="Name">
        </div>
        <div class="form-group">
          <input type="email" class="form-control" id="exampleFormControlInput1" placeholder="Email">
        </div>

        <div class="form-group">
          <textarea class="form-control" id="exampleFormControlTextarea1" rows="4" placeholder="Comment"></textarea>
        </div>

        <div class="form-group">
          <button type="submit" class="btn btn-dark btn-lg btn-block mt-3">Submit</button>
        </div>
      </form>
    </div>
  </div>
</div>
</div>

<%- /* Expose server-rendered data as window.SAILS_LOCALS :: */ exposeLocalsToBrowser() %>
